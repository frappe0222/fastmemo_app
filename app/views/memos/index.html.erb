<%= link_to '新規メモ', '/memos/new' %>

<h1>fastMemo app</h1>
 <% @memos.each do |memo| %>
 <ul>
     <li>
       <%= memo.content %>
       <%= memo.created_at %>
       <%= link_to "編集", edit_memo_path(memo) %>
       <%= link_to "削除", memo_path(memo.id), method: :delete %>
     </li>
 </ul>
<% end %>